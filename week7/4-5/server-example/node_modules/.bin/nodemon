#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules/nodemon/bin/nodemon.js/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules/nodemon/bin/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules/nodemon/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/node_modules:/home/icely/Desktop/wdi-terabyte-work/node_modules:/home/icely/Desktop/node_modules:/home/icely/node_modules:/home/node_modules:/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/nodemon/bin/nodemon.js/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/nodemon/bin/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/nodemon/node_modules" "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules/nodemon/bin/nodemon.js/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules/nodemon/bin/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules/nodemon/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/1.18.10/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/nodemon/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/node_modules:/home/icely/Desktop/wdi-terabyte-work/node_modules:/home/icely/Desktop/node_modules:/home/icely/node_modules:/home/node_modules:/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/nodemon/bin/nodemon.js/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/nodemon/bin/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/nodemon/node_modules" node  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
