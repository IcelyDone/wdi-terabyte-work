#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/fsevents/1.2.7/node_modules/fsevents/node_modules/needle/bin/needle/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/fsevents/1.2.7/node_modules/fsevents/node_modules/needle/bin/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/fsevents/1.2.7/node_modules/fsevents/node_modules/needle/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/fsevents/1.2.7/node_modules/fsevents/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/fsevents/1.2.7/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/fsevents/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules/.registry.npmjs.org/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/server-example/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/4-5/node_modules:/home/icely/Desktop/wdi-terabyte-work/week7/node_modules:/home/icely/Desktop/wdi-terabyte-work/node_modules:/home/icely/Desktop/node_modules:/home/icely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
